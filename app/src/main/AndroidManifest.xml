<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.king.myweb">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".VerMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".FirstActivity"></activity>
        <activity android:name=".ModifyPwdActivity"></activity>
        <activity android:name=".InformationActivity"></activity>
        <activity android:name=".PlanActivity"></activity>
        <activity android:name=".CurriculumActivity"></activity>
        <activity android:name=".ScoreActivity"></activity>
        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>